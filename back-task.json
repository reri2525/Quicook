{
    "family": "quicook-back",
    "containerDefinitions": [
        {
            "name": "quicook-back",
            "image": "public.ecr.aws/v2y9n7g3/quicook-back",
            "cpu": 3072,
            "portMappings": [
                {
                    "name": "quicook-back-3001-tcp",
                    "containerPort": 3001,
                    "hostPort": 3001,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "DB_USERNAME",
                    "value": "admin"
                },
                {
                    "name": "RAILS_ENV",
                    "value": "production"
                },
                {
                    "name": "TZ",
                    "value": "Asia/Tokyo"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "DB_HOST",
                    "valueFrom": "db_host"
                },
                {
                    "name": "DB_PASSWORD",
                    "valueFrom": "db_password"
                },
                {
                    "name": "S3_ACCESS_KEY",
                    "valueFrom": "access_key"
                },
                {
                    "name": "S3_SECRET_ACCESS_KEY",
                    "valueFrom": "secret_access_key"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/quicook-task",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "taskRoleArn": "arn:aws:iam::038836664586:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::038836664586:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "4096",
    "memory": "16384",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}